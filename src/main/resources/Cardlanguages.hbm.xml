<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="classes.Cardlanguages" table="cardlanguages" schema="public" catalog="frikartas">
        <id name="cardLanguageId" column="card_language_id">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="optimizer">none</param>
                <param name="increment_size">1</param>
                <param name="sequence_name">public.cardlanguages_card_language_id_seq</param>
            </generator>
        </id>
        <property name="cardId" column="card_id"/>
        <property name="language" column="language"/>
    </class>
</hibernate-mapping>